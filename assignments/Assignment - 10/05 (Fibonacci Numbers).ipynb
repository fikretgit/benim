    "fibonacci = [1]\n",
    "listem = [0,1]\n",
    "sayaç = 1\n",
    "\n",
    "while fibonacci[-1] <= 55:\n",
    "    sayı = listem[sayaç - 1] + listem[sayaç]\n",
    "    listem.append(sayı)\n",
    "    fibonacci.append(sayı)\n",
    "    sayaç += 1\n",
    "    if sayı == 55:\n",
    "        break\n",
    "    \n",
    "print(fibonacci)\n",
    "    "
